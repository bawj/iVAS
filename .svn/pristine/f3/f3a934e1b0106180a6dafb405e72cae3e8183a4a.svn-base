public enum DeviceTypeEnum {
    /**
     * 常用字段
     */
    TYPE_HUAWEI("Huawei", "华为"),
    TYPE_HK_VISION("HKVision", "海康"),
    TYPE_HUA_WEI_NVR("HuaweiNVR", "华为录像机"),
    TYPE_GB28181PLATFORM("GB28181Platform", "国标28181"),
    ;

    private String status;
    private String msg;

    DeviceTypeEnum(String status, String msg) {
        this.status = status;
        this.msg = msg;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getMsg() {
        return msg;
    }

    public void setMsg(String msg) {
        this.msg = msg;
    }
}
